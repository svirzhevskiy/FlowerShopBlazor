<style>
	.truck{
		fill: #173d40;
	}

	.tier{
		fill:#454545;
		animation: tierSpin 2s linear 0s infinite;
	}
	
	.back-tier{
		transform-origin: 76% 67%;
	}
	
	.front-tier{
		transform-origin: 19% 67%;
		animation-delay: 1s;
	}
	
	.disk{
		fill: #173d40;
	}
	
	@@keyframes tierSpin {
		0% { 
			transform: rotate(360deg);
		}
		100% {
			transform: rotate(0deg); 
		}	
	}
	
	.flower{
		animation: flowerRide 3s linear 0s infinite;
	}
	
	.flower-2{
		animation: flowerRide 3s linear 1.5s infinite;
	}
	
	.flower-3{
		animation: flowerRide 3s linear 0.5s infinite;
	}
	
	.flower-4{
		animation: flowerRide 3s linear 1.2s infinite;
	}
	
	@@keyframes flowerRide {
		0% {
			transform: translate(0);
		}
		25% {
			transform: translate(1%, -1%);
		}
		50% {
			transform: translate(-1%, -1%);
		}
		75% {
			transform: translate(-1%, -0.5%);
		}
		100% {
			transform: translate(0);
		}
	}
</style>